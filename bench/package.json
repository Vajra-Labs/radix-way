{
  "name": "@router/bench",
  "type": "module",
  "private": true,
  "scripts": {
    "bench": "bun run ./src/runner.js",
    "stress": "bun run ./src/stress-test.js",
    "memory": "bun --smol --expose-gc src/memory.js",
    "node:bench": "node ./src/runner.js",
    "node:stress": "node ./src/stress-test.js",
    "node:memory": "node --expose-gc src/memory.js"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "find-my-way": "^9.3.0"
  },
  "packageManager": "bun@1.3.4"
}
