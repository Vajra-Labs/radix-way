{
  "name": "radix-way",
  "version": "1.3.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun remove-dist && bun run build.ts",
    "lint": "eslint \"./{src,tests}/**/*.{ts,js}\" --fix",
    "test": "vitest run",
    "demo": "bun run demo.ts",
    "format": "prettier --write \"./{src,tests}/**/*.{ts,js}\"",
    "release": "bun run build && npm publish",
    "check-types": "tsc --noEmit",
    "coverage": "vitest --run --coverage",
    "remove-dist": "rm -rf dist",
    "bench": "cd bench && bun run bench",
    "memory": "cd bench && bun run memory"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@swc/core": "^1.15.7",
    "@types/node": "^24.10.4",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsdown": "^0.15.12",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "unplugin-swc": "^1.5.9",
    "unplugin-unused": "^0.5.6",
    "vitest": "^4.0.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vajra-Labs/radix-way.git"
  },
  "keywords": [
    "router",
    "radix-tree",
    "radix",
    "trie",
    "http-router",
    "routing",
    "url-router",
    "path-matching",
    "middleware",
    "express",
    "hono",
    "fastify",
    "performance",
    "fast-router",
    "parametric-routes",
    "wildcard",
    "regex-routes",
    "typescript",
    "zero-dependencies",
    "lightweight"
  ],
  "homepage": "https://github.com/Vajra-Labs/radix-way#readme",
  "license": "MIT",
  "author": "Aashish Panchal <aipanchal51@gmail.com>",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.3.4"
}
